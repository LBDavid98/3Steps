#alias {display_desired_summons}{
	#if {"$draw[summoner]" == "on"}{
		#screen clear square $summoner[top] -$summoner[width]+2 $summoner[bottom] -$summoner[width]+40;
		#draw rounded box $summoner[top] -$summoner[width]+2 $summoner[bottom] -$summoner[width]+40;
