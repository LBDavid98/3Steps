			#showme {Current Summon Setup:  [<128>ON<088>]} $summoner[top]+1 -$summoner[width]+5;
			#button {$summoner[top]+1;-$summoner[width]+28;$summoner[top]+1;-$summoner[width]+32}{autos};
		}{
			#showme {Current Summon Setup:  [<118>OFF<088>]} $summoner[top]+1 -$summoner[width]+5;
			#button {$summoner[top]+1;-$summoner[width]+28;$summoner[top]+1;-$summoner[width]+32}{autos};
		};
		#showme {                 Avg:  ${mage_avg[1][avg(damage)]}} $summoner[top]+2 -$summoner[width]+5;
		#showme {       [<afa>+<088>] [<afa>+<088>]  <<afa>next<088>>} $summoner[top]+3 -$summoner[width]+5;
		#button {$summoner[top]+3;-$summoner[width]+12;$summoner[top]+3;-$summoner[width]+14}{tankcountup};
		#button {$summoner[top]+3;-$summoner[width]+16;$summoner[top]+3;-$summoner[width]+18}{tanklevelup};
		#button {$summoner[top]+3;-$summoner[width]+21;$summoner[top]+3;-$summoner[width]+27}{tanknameup};
		#showme {Tank:   $my[summon][desired][tank][count] x $my[summon][desired][tank][level] - $my[summon][desired][tank][name]} $summoner[top]+4 -$summoner[width]+5;
		#showme {       [<faa>-<088>] [<faa>-<088>]  <<faa>prev<088>>} $summoner[top]+5 -$summoner[width]+5;
		#button {$summoner[top]+5;-$summoner[width]+12;$summoner[top]+5;-$summoner[width]+14}{tankcountdown};
		#button {$summoner[top]+5;-$summoner[width]+16;$summoner[top]+5;-$summoner[width]+18}{tankleveldown};
		#button {$summoner[top]+5;-$summoner[width]+21;$summoner[top]+5;-$summoner[width]+27}{tanknamedown};
		#showme {       [<afa>+<088>] [<afa>+<088>]  <<afa>next<088>>} $summoner[top]+6 -$summoner[width]+5;
		#button {$summoner[top]+6;-$summoner[width]+12;$summoner[top]+6;-$summoner[width]+14}{damagecountup};
		#button {$summoner[top]+6;-$summoner[width]+16;$summoner[top]+6;-$summoner[width]+18}{damagelevelup};
		#button {$summoner[top]+6;-$summoner[width]+21;$summoner[top]+6;-$summoner[width]+27}{damagenameup};
		#showme {Damage: $my[summon][desired][damage][count] x $my[summon][desired][damage][level] - $my[summon][desired][damage][name]} $summoner[top]+7 -$summoner[width]+5;
		#showme {       [<faa>-<088>] [<faa>-<088>]  <<faa>prev<088>>} $summoner[top]+8 -$summoner[width]+5;
		#button {$summoner[top]+8;-$summoner[width]+12;$summoner[top]+8;-$summoner[width]+14}{damagecountdown};
		#button {$summoner[top]+8;-$summoner[width]+16;$summoner[top]+8;-$summoner[width]+18}{damageleveldown};
		#button {$summoner[top]+8;-$summoner[width]+21;$summoner[top]+8;-$summoner[width]+27}{damagenamedown};
		#showme {       [<afa>+<088>] [<afa>+<088>]  <<afa>next<088>>} $summoner[top]+9 -$summoner[width]+5;
