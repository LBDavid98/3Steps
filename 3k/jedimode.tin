	#button {58;-40;58;-20}{
		#var {my[desired][weapon]}{none};
		!sheath;
		#var {my[desired][mode]}{hand to hand};
		#var {my[desired][modeshort]}{h};
		!focus hand to hand;
		show_mode_selector;
