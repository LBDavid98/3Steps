#config {repeat enter}{on};
#read .tt/aliases.tin;
#read .tt/graph.tin;

#alias {load3k} {
	#config {packet patch}{0.1};
	#read .tt/3k/ansi.tin;
	#read .tt/3k/mysql.tin;
	#read .tt/3k/mapper.tin;
	#read .tt/3k/mip.tin;
	#read .tt/3k/chat.tin;
	#read .tt/3k/vars.tin;
	#read .tt/3k/aliases.tin;
	#read .tt/3k/assemble.tin;
	#read .tt/3k/corpses.tin;
	#read .tt/3k/gag.tin;
	#read .tt/3k/mob_monitors.tin;
	#read .tt/3k/trigs.tin;
	#read .tt/3k/triggers.tin;
	#read .tt/3k/draw.tin;
	#delay {ansi}{
		redraw;
		#read .tt/3k/ansi_setup.tin
	}{2};
	#script {cat .tt/map/lastroom.tt};
}

#alias {loadAA} {
	#config {packet patch}{0.1};
	#read .tt/AA/mapper.tin;
	#read .tt/AA/killer.tin;
	#read .tt/AA/draw.tin;
	#read .tt/AA/triggers.tin;
	#read .tt/AA/aliases.tin;
	#read .tt/AA/gag.tin;
	#read .tt/AA/vars.tin;
	#read .tt/AA/chat.tin;
	#script {cat .tt/AA/lastroom.tt};
}

#alias {load}{
	#switch {"$mud"}{
		#case {"3k"}{
			load3k;
			resume_chat;
		};
		#case {"3s"}{
			load3k;
			resume_chat;
		};
		#case {"AA"}{
			loadAA;
		};
	};
};

load;

#sub {~\t} {       }
